{
    "name": "peaky-blind3rs/ip-address-interface",
    "description": "IP Address Interface",
    "type": "library",
    "require": {
        "php": ">=8.2.7",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.13",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "PeakyBlind3rs\\IpAddressInterface\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeakyBlind3rs\\IpAddressInterface\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-clover=coverage.xml",
        "cs-check": "phpcs src/ tests/ --colors",
        "static-analysis": "psalm --show-info=false --stats --shepherd"
    },
    "authors": [
        {
            "name": "Tommy Shelby",
            "email": "developers@remitso.com"
        }
    ]
}
